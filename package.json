{
  "name": "react-indicators",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Simple, responsive, canvas-based indicators that you can use to communicate the progress of loaders, timers or whatever else you might need a progress indicator for.",
  "repository": "ryanhefner/react-indicators",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "files": [
    "BarIndicator.js",
    "CircleIndicator.js",
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -f BarIndicator.js && rm -f CircleIndicator.js && rm -fr es && rm -fr umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "dependencies": {
    "clean-react-props": "^0.1.1",
    "lomit": "^0.1.1",
    "npm": "^5.7.1",
    "prop-types": "^15.5.10",
    "react-canvas-wrapper": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "gzip-size": "^3.0.0",
    "jest": "^20.0.4",
    "pretty-bytes": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "keywords": [
    "react",
    "react-component",
    "indicator",
    "indicators",
    "progress",
    "timer",
    "components",
    "library",
    "canvas"
  ]
}

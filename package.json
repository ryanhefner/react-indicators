{
  "name": "react-indicators",
  "version": "0.3.5",
  "license": "MIT",
  "description": "Simple, responsive, canvas-based indicators that you can use to communicate the progress of loaders, timers or whatever else you might need a progress indicator for.",
  "repository": "ryanhefner/react-indicators",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "keywords": [
    "react",
    "react-component",
    "indicator",
    "indicators",
    "progress",
    "timer",
    "components",
    "library",
    "canvas"
  ],
  "files": [
    "BarIndicator.js",
    "CircleIndicator.js",
    "es",
    "index.js",
    "src",
    "umd"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -f index.js && rm -f BarIndicator.js && rm -f CircleIndicator.js && rm -fr es && rm -fr umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __tests__ --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15"
  },
  "dependencies": {
    "lomit": "^0.1.1",
    "prop-types": "^15.5.10",
    "react-canvas-wrapper": "^0.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "greenkeeper-lockfile": "^1.15.1",
    "gzip-size": "^4.1.0",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "pretty-bytes": "^5.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.3.1",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  }
}
